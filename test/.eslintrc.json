{
    "env": {
        "mocha": true
    },
    "globals": {
        "open_window": true
    },
    "rules": {
        "import/no-commonjs": 0,
        "func-names": 0,
        "global-require": 0,
        "max-nested-callbacks": 0,
        "no-eval": 0,
        "no-magic-numbers": 0,
        "no-unused-expressions": 0,
        "prefer-arrow-callback": 0
    }
}
